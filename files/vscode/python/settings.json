{
    "files.defaultLanguage": "python",
    "python.formatting.provider": "black",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/__pycache__": true,
        "**/.pytest_cache": true,
        "**/.mypy_cache": true
    },
    "autoDocstring.quoteStyle": "'''"
}